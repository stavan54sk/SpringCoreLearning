<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        <bean id="user" class="com.testpackage.beans.User" init-method="init" destroy-method="destroy">
        <property name="name" value="XMLNAME" ></property>
        <property name="password" value="XMLPASSWORD"></property>
        <property name="desc" value="XMLDESC"></property>
        </bean>
	</beans>